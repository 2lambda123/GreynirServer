    so_2_þgf_þf_sagnb Nl_þgf Nl_þf     # Konurnar höfðu [ekki] gefið Páli gjöfina
    | so_2_þf_ef_sagnb Nl_þf Nl_ef     # Konurnar höfðu [ekki] krafið Pál svara
    | so_2_þf_þf_sagnb Nl_þf Nl_þf     # Konurnar höfðu [ekki] kostað Pál skildinginn
    | so_2_þf_þgf_sagnb Nl_þf Nl_þgf   # Konurnar höfðu [ekki] kallað Pál öllum illum nöfnum
    | so_2_þgf_þgf_sagnb Nl_þgf Nl_þgf # Konurnar höfðu [ekki] skilað Páli bílnum
    | so_2_þgf_ef_sagnb Nl_þgf Nl_ef   # Konurnar höfðu [ekki] óskað Páli alls hins besta
    | so_2_þgf_nf_sagnb Nl_þgf Nl_nf   # Kristín hafði [ekki] verið Páli stoð og stytta
    | 'gera'_sagnb Nl_þgf LoTengtSögn_nf_et_hk NhFyllingAtv # Konurnar höfðu [ekki] gert Páli kleift að klæða sig

# Það er gott að ná tveimur viðföngum með sögn í sagnbeygingu
$score(+6) Sögn_2_sagnb

# Sögn í nafnhætti með tveimur viðföngum

Sögn_2_nh →
    so_2_þgf_þf_nh Nl_þgf Nl_þf        # Konurnar munu [ekki] gefa Páli gjöfina
    | so_2_þf_ef_nh Nl_þf Nl_ef        # Konurnar munu [ekki] krefja Pál svara
    | so_2_þf_þf_nh Nl_þf Nl_þf        # Konurnar munu [ekki] kosta Pál skildinginn
    | so_2_þf_þgf_nh Nl_þf Nl_þgf      # Konurnar munu [ekki] kalla Pál öllum illum nöfnum
    | so_2_þgf_þgf_nh Nl_þgf Nl_þgf    # Konurnar munu [ekki] skila Páli bílnum
    | so_2_þgf_ef_nh Nl_þgf Nl_ef      # Konurnar munu [ekki] óska Páli alls hins besta
    | so_2_þgf_nf_nh Nl_þgf Nl_nf      # Kristín mun [ekki] vera Páli stoð og stytta
    | 'gera'_nh Nl_þgf LoTengtSögn_nf_et_hk NhFyllingAtv # Konurnar munu [ekki] gera Páli kleift að klæða sig

So_2_nh_þf_nf → so_2_þf_nf_nh
So_2_nh_þf_þf → so_2_þf_þf_nh
So_2_nh_þf_þgf → so_2_þf_þgf_nh
So_2_nh_þf_ef → so_2_þf_ef_nh

So_2_nh_þgf_nf → so_2_þgf_nf_nh
So_2_nh_þgf_þf → so_2_þgf_þf_nh
So_2_nh_þgf_þgf → so_2_þgf_þgf_nh
So_2_nh_þgf_ef → so_2_þgf_ef_nh

# Sögn í boðhætti með tveimur viðföngum

Sögn_2_bh →
    so_2_þgf_þf_bh Nl_þgf Nl_þf        # Gefðu Páli gjöfina
    | so_2_þf_ef_bh Nl_þf Nl_ef        # Krefðu Pál svars
    | so_2_þf_þf_bh Nl_þf Nl_þf        # Kostaðu Pál skildinginn
    | so_2_þf_þgf_bh Nl_þf Nl_þgf      # Kallaðu Pál öllum illum nöfnum
    | so_2_þgf_þgf_bh Nl_þgf Nl_þgf    # Skilaðu Páli bílnum
    | so_2_þgf_ef_bh Nl_þgf Nl_ef      # Óskaðu Páli alls hins besta
    | so_2_þgf_nf_bh Nl_þgf Nl_nf      # Vertu Páli stoð og stytta
    | 'gera'_bh Nl_þgf LoTengtSögn_nf_et_hk NhFyllingAtv # Gerðu Páli kleift að klæða sig í fötin

# Sögn með einu viðfangi ('borðaði matinn')
Sögn_1/tala/pers/kyn →

    # (þf:)  Konurnar borðuðu og meltu [ekki] matinn
    # (þgf:) Konurnar afneituðu, og beinlínis höfnuðu, [ekki] Páli
    # (ef:)  Konurnar nutu [ekki] matarins
    # (nf:)  Konurnar voru [ekki] leikarar

    # (Páll) klappaði og strauk kettinum
    # (Kjósandi) velur með persónkjöri frambjóðendur sem hann kýs
    # (Við) einsetjum okkur að vinna með öðrum þjóðum
    # (Sveitarfélögin) ráða sjálf málefnum sínum
    # (Hún) sagði lafmóð/sjálf söguna
    so_1/fall/tala/pers OgSögn_1/fall/tala/pers* FsRuna? LoTengtSögn_nf/tala/kyn? Nl/fall NhFylling?

    # (Ég tel/segi/álít) Pál vera dagfarsprúðan mann
    | So_1_þf_nh/tala/pers OgSögn_1_þf_nh/tala/pers* SagnarBotn

    | so_mm/tala/pers OgSögn_mm/tala/pers* Nl_þgf                    # (Bankar) reynast og gagnast mér (vel)
    | HjSögn/tala/pers FsAtv? so_mm_sagnb OgSögn_mm_sagnb* Nl_þgf    # (Bankar) hafa reynst og gagnast mér (vel)

    | HjSögnLhÞtSM/tala/pers FsAtv? so_2_þgf_þf_lhþt/tala/kyn Nl_þgf # Konan hefur verið/skal/mun gefin Páli
    | HjSögnLhÞtSM/tala/pers FsAtv? so_2_þf_ef_lhþt/tala/kyn Nl_ef   # Konan hefur verið/skal/mun spurð álits
    | HjSögnLhÞtSM/tala/pers FsAtv? so_2_þf_þgf_lhþt/tala/kyn Nl_þgf # Konan hefur verið/skal/mun kölluð öllum illum nöfnum
    # HjSögnLhÞtSM/tala/pers FsAtv? so_2_þgf_ef_lhþt/tala/kyn Nl_ef  # Konan hefur verið/skal/mun óska alls hins besta
    # HjSögnLhÞtSM/tala/pers FsAtv? so_2_þf_þf_lhþt/tala/kyn Nl_þf   # Konan hefur verið/skal/mun kosta skildinginn

# Býli í landinu eru/geta verið/verða/munu vera um 3.000 [talsins]
Sögn_1_ft/pers/kyn →
    HjSögnLhÞt_ft/pers FsAtv? tala

# Sagnir sem geta staðið með þf + nh
# '(Ég) álít (Pál vera dagfarsprúðan mann)'
# '(Guðrún) lét (soninn hafa bílinn)'
# '(Við) sáum (strákinn vaxa úr grasi)'
# '(Jón) kvað (stíginn vera öruggan)'
# '(Myndbandið) sýndi (björgunarmenn bera hann inn í sjúkrabíl)'
So_1_þf_nh/tala/pers →
    'segja'/tala/pers
    | 'telja'/tala/pers
    | 'láta'/tala/pers
    | 'álíta'/tala/pers
    | 'sjá'/tala/pers
    | 'kveða'/tala/pers
    | 'sýna'/tala/pers

So_1_þf_nh →
    'segja'_nh
    | 'telja'_nh
    | 'láta'_nh
    | 'álíta'_nh
    | 'sjá'_nh
    | 'kveða'_nh
    | 'sýna'_nh

So_1_þf_nh_sagnb →
    'segja'_gm_sagnb
    | 'telja'_gm_sagnb
    | 'láta'_gm_sagnb
    | 'álíta'_gm_sagnb
    | 'sjá'_gm_sagnb
    | 'kveða'_gm_sagnb
    | 'sýna'_gm_sagnb

$score(+2) So_1_þf_nh_sagnb

SagnarBotn →
    # (Páll segir) mistökin strax valda vanda
    Nl_þf AtvFs? NhRuna
    # (Þingmaðurinn telur) niðurstöðuna vera ranga / (Verkfræðingurinn segir) útkomuna verða tvísýna
    | Nl_þf/tala/kyn AtvFs? HjSögnVeraVerða LoLiður_nf/tala/kyn

    # (Páll segir) mistökin [ekki] hafa verið gerð (á vakt Jóns)
    # (Páll segir) mistökin [ekki] hafa verið (í uppleggi Friðgeirs)
    # Hér er notað HjSögnEkkiVera í stað HjSögnVera til að forðast margræðni
    # gagnvart NhRunu
    | Nl_þf/tala/kyn AtvFs? HjSögnEkkiVera so_lhþt/tala/kyn?

    # (Hann segir) hvern farþega [strax] hafa fengið tíu evrur / hafa farið að grilla
    | Nl_þf/tala/kyn AtvFs? 'hafa'_nh HreinSögn_sagnb/tala/kyn # !!! Hér vantar að senda þolfall: 'galvaskan að grilla'

    # (Páll segir) Ásu [ekki] vera/hafa verið góðan stjórnanda
    | Nl_þf/tala AtvFs? HjSögnEkkiVera Nl_þf/tala

    # (Páll segir) skorta [á] (að farið sé að lögum)
    | so_subj_nh_none NhFylling?
    # (Páll segir) tefjast (að hefja framkvæmdir)
    | so_mm_nh NhFylling?
    # (Már segir) misskilnings gæta (hjá Hannesi)
    | Nl_ef AtvFs? so_subj_nh_ef
    # (Anna telur) fólki fækka í landinu
    | Nl_þgf AtvFs? so_subj_nh_þgf
    # (Már segir) [í grundvallaratriðum] gæta misskilnings (hjá Hannesi)
    | FsAtv? so_subj_nh_ef Nl_ef
    # !!! Ekki virðist þörf fyrir öfuga orðaröð í þgf eða þf

# Sögn með einu viðfangi sem stendur í undirsetningu
# ('Konurnar sem borðuðu matinn')
ÖfugSögn_1/tala/pers/kyn →

    # (þf:)  Konurnar sem borðuðu [ekki] matinn
    # (þgf:) Konurnar sem höfnuðu [ekki] Páli
    # (ef:)  Konurnar sem nutu [ekki] matarins
    # (nf:)  Konurnar sem voru [ekki] leikarar

    So_1_sagnb/fall HjSögn/tala/pers Nl/fall LoTengtSögn_nf/tala/kyn?  # Konurnar sem borðað höfðu matinn [sjálfar/galvaskar]
    | So_1_nh/fall HjSögnNh/tala/pers Nl/fall LoTengtSögn_nf/tala/kyn? # Konurnar sem borða munu matinn [allar/galvaskar]

    | Nl/fall AtvFs? so_1/fall/tala/pers LoTengtSögn_nf/tala/kyn?      # Konurnar sem matinn [snarlega] borðuðu [báðar/galvaskar]

    | so_2_þgf_þf_lhþt/tala/kyn HjSögnLhÞtSM/tala/pers Nl_þgf          # Konan sem gefin hafði verið/skal/mun Páli
    | Nl_þgf HjSögnLhÞtSM/tala/pers FsAtv? so_2_þgf_þf_lhþt/tala/kyn   # Konan sem Páli hafði verið/skal/mun gefin

    | so_2_þf_þgf_lhþt/tala/kyn HjSögnLhÞtSM/tala/pers Nl_þgf          # Konan sem kölluð hafði verið/skal/mun illum nöfnum
    | Nl_þgf HjSögnLhÞtSM/tala/pers FsAtv? so_2_þf_þgf_lhþt/tala/kyn   # Konan sem illum nöfnum hafði verið/skal/mun kölluð

    | so_2_þf_ef_lhþt/tala/kyn HjSögnLhÞtSM/tala/pers Nl_ef            # Konan sem spurð hafði verið/skal/mun álits
    | Nl_ef HjSögnLhÞtSM/tala/pers FsAtv? so_2_þf_ef_lhþt/tala/kyn     # Konan sem álits hafði verið/skal/mun spurð

    | SoFrumlag_1_þf_nh FsAtv? NhRuna                                  # Orðin sem Páll lét [ekki] falla
    # so_2_þgf_ef, so_2_þgf_þgf og so_2_þf_þf virðast ekki geta staðið
    # í lhþt heldur aðeins sem sagnbót

SoFrumlag_1_þf_nh →
    Nl_nf/tala/pers So_1_þf_nh/tala/pers  # (Orðin sem) Páll lét (falla) / (Orðin sem) þú lést (falla)

SögnMeðF_1/tala/pers/kyn →

    # (þf:)  (Í dag) borðuðu konurnar [ekki] matinn [sjálfar/galvaskar]
    # (þgf:) (Í dag) afneituðu og höfnuðu konurnar [ekki] Páli [báðar/galvaskar]
